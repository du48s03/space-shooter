        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Check keyboard
var num_options = array_length_1d(options);
if(focus &lt; num_options-2){
    if(keyboard_check_pressed(vk_down)){
        focus = (focus+1)%num_options;
        audio_play_sound(snd_change_option, 80, false);
    }
    if(keyboard_check_pressed(vk_up)){
        focus -=1;
        if(focus&lt;0) focus = num_options-2;
        audio_play_sound(snd_change_option, 80, false);
    }
    
}
else if(focus == num_options-2){//cancel
    if(keyboard_check_pressed(vk_right)|| keyboard_check_pressed(vk_left))    {focus = num_options-1;audio_play_sound(snd_change_option, 80, false);}
    if(keyboard_check_pressed(vk_up))    {focus = num_options-3;audio_play_sound(snd_change_option, 80, false);}
    if(keyboard_check_pressed(vk_down))    {focus = 0;audio_play_sound(snd_change_option, 80, false);}
    
}
else if(focus == num_options-1){//confirm
    if(keyboard_check_pressed(vk_right)|| keyboard_check_pressed(vk_left))    {focus = num_options-2;audio_play_sound(snd_change_option, 80, false);}
    if(keyboard_check_pressed(vk_up))    {focus = num_options-3;audio_play_sound(snd_change_option, 80, false);}
    if(keyboard_check_pressed(vk_down))    {focus = 0;audio_play_sound(snd_change_option, 80, false);}
}
/*
if(keyboard_check_pressed(vk_space)){
    show_message(string(num_options)+","+string(focus));
}
*/
</string>
